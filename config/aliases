e.=explorer .
gl=git log --oneline --all --graph --decorate  $*
ls=ls --show-control-chars -F --color $*
pwd=cd
clear=cls
history=cat %CMDER_ROOT%\config\.history
unalias=alias /d $1
vi=vim $*


ff=find . -type f -name $*
fd=find . -type d -name $*

hosts=notepad.exe C:\Windows\System32\drivers\etc\hosts
aliases=notepad.exe %CMDER_ROOT%\config\aliases
paths=notepad.exe %CMDER_ROOT%\vendor\init.bat
commands=notepad.exe %CMDER_ROOT%\commands.txt
paths=notepad.exe %CMDER_ROOT%\vendor\init.bat
machine=notepad.exe %CMDER_ROOT%\machine_init.bat
khelp = grep -A 1 $1 %CMDER_ROOT%\commands.txt

gpdm=git push dokku master
gpd=git push dokku master
gpo=git push origin master

speedtest=wget -O nul http://speedtest.wdc01.softlayer.com/downloads/test500.zip
extip=curl icanhazip.com

namebench=python %CMDER_ROOT%\vendor\namebench-1.3.1\namebench.py
heroku = %CMDER_ROOT%\vendor\Heroku\bin\heroku.bat $*

nc = ncat $*
nodejs=node 

git-root=cd $(git rev-parse --show-cdup)

gitsome= curl -s http://whatthecommit.com/index.txt
